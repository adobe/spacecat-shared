  "dependencies": {
    "@adobe/fetch": "4.1.11",
    "@adobe/helix-universal": "5.0.8",
    "@adobe/spacecat-shared-utils": "1.26.4",
    "@adobe/spacecat-shared-ims-client": "1.5.6"
  },
  "devDependencies": {
    "chai": "5.2.0",
    "chai-as-promised": "8.0.1",
    "mocha": "10.3.0",
    "mocha-multi-reporters": "1.5.1",
