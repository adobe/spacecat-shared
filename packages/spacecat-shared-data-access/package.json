  "dependencies": {
    "@adobe/spacecat-shared-dynamo": "1.3.47",
    "@adobe/spacecat-shared-utils": "1.22.4",
    "@aws-sdk/client-dynamodb": "3.693.0",
    "@aws-sdk/lib-dynamodb": "3.693.0",
    "@types/joi": "17.2.3",
    "aws-xray-sdk": "3.10.2",
    "electrodb": "3.0.1",
    "joi": "17.13.3",
