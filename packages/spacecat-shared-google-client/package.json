{
  "name": "@adobe/spacecat-shared-google-client",
  "version": "1.4.4",
  "description": "Shared modules of the Spacecat Services - Google Client",
  "type": "module",
  "engines": {
    "node": ">=22.0.0 <23.0.0",
    "npm": ">=10.9.0 <12.0.0"
  },
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
