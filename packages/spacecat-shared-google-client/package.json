    "@adobe/spacecat-shared-http-utils": "1.9.4",
    "@adobe/spacecat-shared-utils": "1.26.4",
    "@aws-sdk/client-secrets-manager": "3.726.1",
    "aws-xray-sdk": "3.10.2",
    "google-auth-library": "9.15.0",
    "googleapis": "144.0.0"
