{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>adobe/spacecat-shared"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "semantic-release"
      ],
      "allowedVersions": ">=23.0"
    },
    {
      "matchPackageNames": [
        "dynamo-db-local"
      ],
      "allowedVersions": "<10.0.0"
    },
    {
      "matchPackageNames": [
        "eslint"
      ],
      "allowedVersions": "<9.0.0"
    }
  ]
}
